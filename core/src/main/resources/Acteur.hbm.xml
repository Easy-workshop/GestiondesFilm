<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.elephorm.formation.entity.Acteur" table="ACTOR">
        <id name="id" column="ID">
            <generator class="identity">
                
            </generator>
        </id>
        <property name="nom" column="LASTNAME"/>
        <property name="prenom" column="FIRSTNAME"/>
        
    </class>

</hibernate-mapping>